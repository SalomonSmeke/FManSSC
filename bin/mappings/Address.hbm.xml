<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 5.1//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-5.1.dtd">

<hibernate-mapping package="com.FManSSC.model.backbones">
	<class name="Address" table="address">
		<id name="id" type="long" column="id"/>
		<property name="street" type="string" column="street"/>
		<property name="streetNo" type="integer" column="streetNo"/>	
		<property name="city" type="string" column="city"/>
		<property name="state" type="string" column="state"/>
		<property name="zip" type="string" column="zip"/>
	</class>
</hibernate-mapping>